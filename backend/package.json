{
  "name": "@raco-hotels/backend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev --local",
    "build": "tsc --noEmit",
    "typecheck": "tsc --noEmit --pretty false",
    "db:generate": "drizzle-kit generate",
    "db:migrate:apply": "wrangler d1 migrations apply local-db --local",
    "lint": "eslint . --ext .ts,.tsx,.js",
    "lint:fix": "eslint . --ext .ts,.tsx,.js --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "dependencies": {
    "@hono/swagger-ui": "^0.5.2",
    "@hono/zod-openapi": "^1.1.0",
    "@noble/hashes": "^1.8.0",
    "cookie": "^0.6.0",
    "drizzle-orm": "^0.33.0",
    "hono": "^4.6.9",
    "jsonwebtoken": "^9.0.2",
    "zod": "^4.0.16"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250704.0",
    "@types/cookie": "^0.6.0",
    "@types/jsonwebtoken": "^9.0.7",
    "drizzle-kit": "^0.24.0",
    "typescript": "^5.5.4",
    "wrangler": "^3.80.3"
  }
}
