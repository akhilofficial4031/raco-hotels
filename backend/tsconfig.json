{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "WebWorker", "DOM.Iterable"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "types": ["@cloudflare/workers-types"],
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "allowJs": true
  },
  "include": ["src", "drizzle/**/*.ts"]
}
